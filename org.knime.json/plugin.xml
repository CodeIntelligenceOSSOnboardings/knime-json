<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.4"?><plugin>

   <!-- This is an example for providing an own category--> 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/16x16/category_json.png"
            level-id="json"
            name="JSON"
            path="/struct-data"/>
   </extension>
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.reader.JSONReaderNodeFactory" />
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.writer.JSONWriterNodeFactory"  after="org.knime.json.node.reader.JSONReaderNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.fromstring.StringToJSONNodeFactory"  after="org.knime.json.node.writer.JSONWriterNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.fromtable.TableToJsonNodeFactory"  after="org.knime.json.node.fromstring.StringToJSONNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.totable.JSONToTableNodeFactory"  after="org.knime.json.node.fromtable.TableToJsonNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.tojson.ColumnsToJsonNodeFactory"  after="org.knime.json.node.totable.JSONToTableNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.fromxml.XMLToJSONNodeFactory"  after="org.knime.json.node.tojson.ColumnsToJsonNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.toxml.JSONToXMLNodeFactory"  after="org.knime.json.node.fromxml.XMLToJSONNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.jsonpath.multi.JSONPathNodeFactory"  after="org.knime.json.node.toxml.JSONToXMLNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.jsonpath.dict.JSONPathDictNodeFactory"  after="org.knime.json.node.jsonpath.multi.JSONPathNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.combine.column.ColumnCombineJsonNodeFactory"  after="org.knime.json.node.jsonpath.dict.JSONPathDictNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.combine.row.RowCombineJsonNodeFactory"  after="org.knime.json.node.combine.column.ColumnCombineJsonNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.combine.row.CombineAndWriteJsonNodeFactory"  after="org.knime.json.node.combine.row.RowCombineJsonNodeFactory"/>
      <!--deprecated <node category-path="/struct-data/json" factory-class="org.knime.json.node.jsonpath.JSONPathNodeFactory"  after="org.knime.json.node.fromxml.XMLToJSONNodeFactory"/>-->
      <!--never released node category-path="/struct-data/json" factory-class="org.knime.json.node.jsonpointer.JSONPointerNodeFactory"  after="org.knime.json.node.jsonpath.JSONPathNodeFactory"/-->
      <!--node category-path="/struct-data/json" factory-class="org.knime.json.node.patch.apply.JSONPatchApplyNodeFactory"  after="org.knime.json.node.jsonpointer.JSONPointerNodeFactory"/-->
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.patch.apply.JSONPatchApplyNodeFactory"  after="org.knime.json.node.combine.row.CombineAndWriteJsonNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.schema.check.JSONSchemaCheckNodeFactory"  after="org.knime.json.node.patch.apply.JSONPatchApplyNodeFactory"/>
      <node category-path="/struct-data/json" factory-class="org.knime.json.node.patch.create.JSONPatchCreateNodeFactory"  after="org.knime.json.node.schema.check.JSONSchemaCheckNodeFactory" />
      <node
            category-path="/struct-data/json"
            factory-class="org.knime.json.node.servicetableoutput.ServiceTableOutputNodeFactory">
      </node>
      <node
            category-path="/struct-data/json"
            factory-class="org.knime.json.node.input.JSONInputNodeFactory"
            >
      </node>
      <node
            category-path="/struct-data/json"
            factory-class="org.knime.json.node.output.JSONOutputNodeFactory"
            >
      </node>
      <node
            category-path="/struct-data/json"
            factory-class="org.knime.json.node.servicein.ServiceTableInputNodeFactory">
      </node>
      <node
            category-path="/struct-data/json"
            factory-class="/org.knime.json.node.servicevariableinput.ServiceVariableInputNodeFactory"
      </node>
      </node>
   </extension>
   <extension
           point="org.knime.workbench.repository.registeredFileExtensions">
      <FileExtensionMapping
              NodeFactory="org.knime.json.node.reader.JSONReaderNodeFactory">
         <FileExtension extension=".json"/>
         <FileExtension extension=".json.gz"/>
      </FileExtensionMapping>
   </extension>
   <extension
         point="org.knime.core.DataValueRenderer">
      <dataValue
            valueClass="org.knime.core.data.json.JSONValue">
         <renderer
               rendererFactoryClass="org.knime.core.data.json.JSONValueRenderer$Factory"
               suggestAsDefault="true">
         </renderer>
      </dataValue>
   </extension>
   <extension
         point="org.knime.core.DataType">
      <DataType
            cellClass="org.knime.core.data.json.JSONCell"
            factoryClass="org.knime.core.data.json.JSONCellFactory">
         <serializer
               cellClass="org.knime.core.data.json.JSONCell"
               serializerClass="org.knime.core.data.json.JSONCell$JSONSerializer">
         </serializer>
         <serializer
               cellClass="org.knime.core.data.json.JSONBlobCell"
               serializerClass="org.knime.core.data.json.JSONBlobCell$JSONSerializer">
         </serializer>
      </DataType>
   </extension>
</plugin>
