<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.knime.testing.TestcaseCollector">
      <TestcaseCollector
            TestcaseCollector="org.knime.json.JSONTestcaseCollector">
      </TestcaseCollector>
   </extension>
   <extension
         point="org.knime.base.AggregationOperator">
